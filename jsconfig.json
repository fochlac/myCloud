{
  "compilerOptions": {
    "target": "esnext",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "paths": {
      "COMPONENTS/*": ["web/components/*"],
      "RAW/*": ["web/components/raw/*"],
      "DIALOG/*": ["web/components/dialog/*"],
      "CONNECTED/*": ["web/components/connected/*"],
      "PAGES/*": ["web/components/views/*"],
      "UTILS/*": ["web/utils/*"],
      "STORE/*": ["web/store/*"],
      "SW/*": ["node_modules/serviceworker-webpack-plugin/lib/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
