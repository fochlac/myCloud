      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.(ts|js)$": "babel-jest"
    },
    "testMatch": [
      "**/__tests__/*.spec.ts"
    ],
    "moduleNameMapper": {
      "^COMPONENTS(.*)$": "<rootDir>/web/components$1",
      "^RAW(.*)$": "<rootDir>/web/components/raw$1",
      "^DIALOG(.*)$": "<rootDir>/web/components/dialog$1",
      "^CONNECTED(.*)$": "<rootDir>/web/components/connected$1",
