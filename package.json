{
  "name": "gallery",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build_client": "node build.js",
    "watch": "node build.js --serve",
    "start": "npm-run-all -p start_server watch",
    "start_server": "nodemon --exec ts-node -- src/index.ts",
    "build_server": "rm -rf ./dist && tsc && chmod +x ./dist/index.js",
    "build": "npm-run-all -p build_client build_server",
    "test": "jest --coverage"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "clone": "^2.1.2",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "exifr": "^7.1.3",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "jsonwebtoken": "^8.3.0",
    "mime-types": "^2.1.20",
    "multer": "^1.3.1",
    "path": "^0.12.7",
    "redux-immutable": "^4.0.0",
    "sharp": "^0.33.5",
    "web-push": "^3.3.1",
    "x-xss-protection": "^1.1.0"
  },
  "devDependencies": {
    "@types/clone": "^2.1.4",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/jest": "^22.2.3",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/multer": "^1.3.7",
    "@types/node": "^10.1.3",
    "autoprefixer": "^8.5.1",
    "codecov": "^3.0.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "esbuild": "^0.24.2",
    "esbuild-copy-static-files": "^0.1.0",
    "esbuild-style-plugin": "^1.6.3",
    "eslint": "^8.57.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "2.12.1",
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.2",
    "jest": "^23.0.1",
    "jest-junit": "^4.0.0",
    "less": "^4.2.1",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.14.0",
    "react-dom": "^16.4.0",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^7.2.9",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "sinon": "^5.0.10",
    "source-map-support": "^0.5.6",
    "ts-node": "^6.2.0",
    "tsconfig-paths": "^3.5.0",
    "typescript": "^5.7.2",
    "typescript-eslint-parser": "^15.0.0",
    "unexpected": "^10.38.0",
    "unexpected-immutable": "^0.2.6",
    "unexpected-sinon": "^10.10.1"
  },
  "nodemonConfig": {
    "ignore": [
      "storage/**/*"
    ]
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.(ts|js)$": "babel-jest"
    },
    "testMatch": [
      "**/__tests__/*.spec.ts"
    ],
    "moduleNameMapper": {
      "^COMPONENTS(.*)$": "<rootDir>/web/components$1",
      "^RAW(.*)$": "<rootDir>/web/components/raw$1",
      "^CONNECTED(.*)$": "<rootDir>/web/components/connected$1",
      "^UTILS(.*)$": "<rootDir>/web/utils$1",
      "^PAGES(.*)$": "<rootDir>/web/components/views$1",
      "^STORE(.*)$": "<rootDir>/web/store$1",
      "^SERVER(.*)$": "<rootDir>/src$1",
      "^CONTROLLER(.*)$": "<rootDir>/src/router/controller$1",
      "\\.(css|less)$": "<rootDir>/test/styleMock.js"
    },
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ],
    "setupFiles": [
      "./test/jest.js"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testURL": "https://food.fochlac.com/test123"
  },
  "browserslist": [
    "last 3 versions",
    "> 1%"
  ]
}
